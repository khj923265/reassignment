<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.rb.rbassignment.repository.MemberRepository">
    <insert id="save" parameterType="com.rb.rbassignment.domain.Member">
        insert into member (id, email, password, name, tel) values (get_seq('member'), #{email}, #{password}, #{name}, #{tel})
    </insert>
</mapper>